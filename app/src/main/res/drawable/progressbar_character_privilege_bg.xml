<!--      设置背景色-->
    <item android:id="@android:id/background"
        android:drawable="@drawable/progress_bar_white_bg">
    </item>



    <!--  设置进度条颜色  -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>

                    <gradient

                        android:type="linear"
                        android:useLevel="true"
                        android:startColor="#946261"
                        android:endColor="#946261" android:angle="360" />

                <corners android:radius="50dp"/>
            </shape>
        </clip>
    </item>

</layer-list>